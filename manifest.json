{
  "name": "Photospere Filter",
  "description": "Hides Photos in Google Maps profile that's not a 360Â° View",
  "version": "1.0",
  "manifest_version": 3,

  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
